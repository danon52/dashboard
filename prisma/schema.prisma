// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        Int    @id @default(autoincrement())
  name      String
  FirstName String
  LastName  String
  age       Int
  email     String
  password  Int
}

model Sales {
  id    Int @id @default(autoincrement())
  prise Int
}
